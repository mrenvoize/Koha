[% USE Koha %]
[% SET KOHA_VERSION = Koha.Preference('Version') %]
[% INCLUDE 'doc-head-open.inc' %]
[% IF ( caller = 'hold-transfer' ) %]
<title>Koha &rsaquo; Circulation &rsaquo; Hold transfer print receipt</title>
[% ELSIF ( caller == 'transfer' ) %]
